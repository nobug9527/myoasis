@using (Html.BeginForm("editnote", "Note",FormMethod.Post))
{
    <input type="hidden" name="id" value="@ViewBag.noteid"/>
    <textarea name="notecontent" id="notecontent"></textarea>
    <script type="text/javascript">
        //console.log("@ViewBag.str");
        CKEDITOR.replace("notecontent");
        CKEDITOR.instances.notecontent.setData('<p>Some other editor data.</p>', {
            callback: function () {
                console.log("<p>123</p>");
                $.ajax({
                    url: "/Note/getcontentedit" + "?id=" + @ViewBag.noteid,
                    type: "post",
                    async: true,
                    success: function (result) {
                        CKEDITOR.instances.notecontent.setData(""+result+"");
                        console.log(""+result+"");
                    }
                })
            }
        });
     
    </script>
    <button type="submit" class="btn btn-primary">提交</button>
}